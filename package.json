{
  "name": "rhythm",
  "author": "nnnnusui",
  "version": "4.0.0",
  "description": "Rhythm games that run in the web browser.",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "plopTemplates": "pnpm cross-env NODE_OPTIONS='--import tsx' plop --plopfile=script/plopfile.ts",
    "clean": "node --import tsx script/clean-up.ts",
    "dev": "pnpm /^dev:.*/",
    "dev:server": "vinxi dev --host",
    "dev:rm-hcm": "nodemon --ext scss --watch src --exec 'node --import tsx script/remove-unused-hcm.ts'",
    "lint": "pnpm /^lint:.*/",
    "lint:eslint": "eslint . --flag unstable_ts_config --max-warnings 0",
    "lint:stylelint": "stylelint src/**/*.{css,scss,sass}",
    "build": "vinxi build",
    "start": "vinxi start",
    "test": "pnpm /^test:.*/",
    "test:spec": "pnpm vitest run",
    "test:tsc": "tsc -b",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@solid-primitives/bounds": "^0.0.123",
    "@solid-primitives/deep": "^0.3.0",
    "@solid-primitives/raf": "^2.2.0",
    "@solid-primitives/resize-observer": "^2.0.26",
    "@solid-primitives/scheduled": "^1.4.4",
    "@solid-primitives/scroll": "^2.0.23",
    "@solid-primitives/storage": "^4.2.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.2",
    "@solidjs/start": "^1.0.10",
    "clsx": "^2.1.1",
    "solid-js": "^1.9.3",
    "ts-deepmerge": "^7.0.2",
    "ui7": "^0.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@solidjs/testing-library": "^0.8.10",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@stylistic/stylelint-plugin": "^3.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__eslintrc": "^2.1.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/eslint-plugin": "^1.1.21",
    "@vitest/ui": "^2.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-access": "^2.2.2",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "happy-css-modules": "^3.2.0",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.9",
    "plop": "^4.0.1",
    "postcss": "^8.4.49",
    "stylelint": "^16.12.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vinxi": "^0.4.3",
    "vite": "^5.4.10",
    "vite-plugin-happy-css-modules": "^0.3.0",
    "vite-plugin-solid": "^2.11.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "homepage": "https://github.com/nnnnusui/Rhythm#readme",
  "bugs": {
    "url": "https://github.com/nnnnusui/Rhythm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nnnnusui/Rhythm.git"
  },
  "private": "true"
}
